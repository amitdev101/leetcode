{
    "manifest_version": 3,
    "name": "Command Executor",
    "version": "1.0",
    "description": "A Chrome extension to execute commands from Django server.",
    "permissions": [
        "activeTab",
        "http://localhost:8000/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
